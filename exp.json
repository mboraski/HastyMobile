{
  "name": "Hasty Mobile App",
  "description": "An instant delivery retail app",
  "slug": "hasty-mobile",
  "privacy": "public",
  "sdkVersion": "24.0.0",
  "version": "1.0.2",
  "orientation": "portrait",
  "primaryColor": "#f5a624",
  "icon": "./src/assets/icons/HastyOrangeIcon.png",
  "splash": {
    "backgroundColor": "#f5a624",
    "resizeMode": "cover",
    "image": "./src/assets/splash.png"
  },
  "packagerOpts": {
    "assetExts": ["ttf", "mp4"]
  },
  "ios": {
    "bundleIdentifier": "com.spartanspeedofservice.myhasty",
    "loadJSInBackgroundExperimental": true,
    "supportsTablet": true
  },
  "android": {
    "package": "com.spartanspeedofservice.hasty"
  }
}
